	"description": "Upload handler",
	"homepage": "http://hypejunction.com",
	"keywords": [
		"elgg", "plugin", "uploads"
	],
	"license": "GPL-2.0",
	"authors": [
		{
			"name": "Ismayil Khayredinov",
			"email": "ismayil.khayredinov@gmail.com"
		}
	],
	"extra": {
		"installer-name": "hypeFilestore"
	},
		"require": {
		"php": ">=5.4",
		"ext-json": "*",
		"ext-xml": "*",
		"ext-curl": "*",
		"composer/installers": "1.*",
		"smottt/wideimage": "dev-master#658e6b8699255724589e8fb86959306ec64af318"
